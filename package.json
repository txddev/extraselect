{
  "name": "@txd/extraselect",
  "files": [
    "dist",
    "lib"
  ],
  "main": "./dist/extraselect.umd.cjs",
  "module": "./dist/extraselect.js",
  "exports": {
    ".": {
      "import": "./dist/extraselect.js"
    },
    "./wrapped": {
      "import": "./dist/wrapped/extraselect.js"
    },
    "./bundle": {
      "import": "./dist/bundle/extraselect.js"
    },
    "./css/": {
      "import": "./dist/css/",
      "require": "./dist/css/"
    },
    "./lib/": {
      "import": "./lib/"
    }
  },
  "private": false,
  "version": "1.10.6",
  "type": "module",
  "scripts": {
    "check": "vue-tsc --noEmit",
    "dev": "vite --host",
    "build": " vite build && vite build -c wrapper.config.ts && vite build -c styles.config.ts && vite build -c bundle.config.ts",
    "build:styles": " vite build -c styles.config.ts",
    "build:wrapper": " vite build -c wrapper.config.ts",
    "build:es": " vite build",
    "styles": "vite build -c styles.config.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "@txd/utils": "github:txddev/js-utils",
    "@vueuse/core": "^11.1.0",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "sass": "^1.79.3",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vue-tsc": "^2.1.6"
  }
}
